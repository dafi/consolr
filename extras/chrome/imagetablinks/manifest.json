{
  "name": "Imagetab Links",
  "version": "1.0",
  "description": "Allow many operations on tab links",
  "background_page": "background.html",
  "browser_action": {
    "default_title": "Imagetab Links",
    "default_icon": "images/tb16.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["js/imagetablinks.js", "js/content.js"]
    }
   ],
  "permissions": [
    "tabs",
    "notifications",
    "http://*/*", "https://*/*"
  ],
  "options_page": "options.html",
   "icons": {
      "48": "images/appicon.png",
      "32": "images/appicon.png",
      "16": "images/appicon.png"
   }
}
